{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "compilerOptions": {
    "target": "es6",
    "lib": ["es2015", "dom"],
    "types": ["cypress"],
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@pages/*": ["cypress/pages/*"],
      "@support/*":["cypress/support/*"],
    }
  },
  "include": ["cypress/**/*.ts", "cypress/**/*.d.ts"]
}